(GeneralizeNumber
  [LAMBDA (x)                                                (* edited%: "25-MAR-81 12:31")
    (COND
       [(LISTP x)
        (MAPCAR x (FUNCTION (LAMBDA (Z)
                              (COND
                                 ((RandomP)
                                  (GeneralizeNumber Z))
                                 (T Z]
       [(FIXP x)
        (CADDR (SETQ UDiff (LIST x RArrow (RAND x (COND
                                                     ((ILEQ x 1000)
                                                      1000)
                                                     (T (TIMES x 10]
       [(NUMBERP x)
        (CADDR (SETQ UDiff (LIST x RArrow (QUOTIENT (RAND (FIX (TIMES x 200))
                                                          (FIX (TIMES x (MAX 5.0 x)
                                                                      200)))
                                                 200.0]
       (T NIL])

