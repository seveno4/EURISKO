       ((MOVD 'AND 'AND-2 T)
        (MOVD 'AND 'AND-1 T)
        (MOVD 'AND 'AND-1 T)
        (MOVD 'BestSubset 'BestSubset-3 T)
        (MOVD 'BestSubset 'BestSubset-2 T)
        (MOVD 'BestSubset 'BestSubset-1 T)
        (MOVD 'AND 'AND-2 T)
        (MOVD 'AND 'AND-1 T)))

