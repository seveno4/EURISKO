  [LAMBDA (Verbo EternalFlg)                             (* ; "Edited 30-Nov-2023 06:50 by seveno4")
    (COND
       ((FIXP Verbo)
        (SETQ Verbosity Verbo))
       (T NIL))
    (PRIN1 "


				Starting EURISKO



Douglas B. Lenat
February, 1981

")
    (InitializeEurisko)
    (SETQ TaskNum 0)
    (CPRIN1 -1 CRLF "Ready to start? ")
    (COND
       ((YesNo)
        (START EternalFlg))
       (T "Type (START) when you are ready."])

