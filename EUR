  [LAMBDA (ov)                                               (* edited%: "22-APR-81 15:15")
    (for i from 0 to (RAND 0 (SQUARE (RAND 1 10)))
       collect (FavorFirst '(RandomChoose (CacheExamples 'Anything))
                      '(GetAStruc])

